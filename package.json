{
  "name": "symfony-helper",
  "version": "0.1.2",
  "description": "Awesome symfony helper tool",
  "main": "./lib/index.js",
  "preferGlobal": "true",
  "scripts": {
    "test": "mocha",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*",
    "coveralls": "npm bin /istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/* && node node_modules/coveralls/bin/coveralls.js < coverage/lcov.info"
  },
  "bin": {
    "symhelp": "bin/test-module.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeldegroot/symfony-helper"
  },
  "keywords": [
    "symfony",
    "cli"
  ],
  "author": {
    "name": "michaeldegroot"
  },
  "license": "MIT",
  "dependencies": {
    "colors": "^1.1.2",
    "fs-extra": "^0.30.0",
    "inquirer": "^1.0.0",
    "jsonfile": "^2.2.3",
    "moment": "^2.12.0",
    "shelljs": "^0.7.0"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "assert-plus": "^1.0.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
